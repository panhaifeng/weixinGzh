.mainInner{
	width: 960px;
	min-height: 372px;
	padding-top: 40px;
	margin: 0 auto;
	overflow: hidden;
	position: relative;
}

.leftMemo{
	position: relative;
	margin-right: 395px;
}

.leftMemo .pictuer {
	position: absolute;
	left: 0;
	right: 0;
	top: 0;
	height: 400px;
	_width: 100%;
	background-repeat: no-repeat;
	background-position: right center;
	/*_ background-image: none; *//* IE6 */
}

.leftMemo .text {
	position: relative;
	font-size: 14px;
	font-weight: normal;
	line-height: 24px;
}

.login_title{
	line-height: 30px;
	margin-top: 15px;
	padding-bottom: 10px;
	font-size: 24px;
	font-weight: normal;
	color: #6f95c8;
}

.leftMemo .text p{
	margin: 0px;
	display: block;
}


.login_pictures_info{
	margin-top: 6px;
	font-size: 12px;
}

.rightBox{
	float: right;
	border: 1px solid #a0b1c4;
	background-color: #fff;
	position: absolute;
	top:65px;
	right: 250px;
	z-index: 12;
	padding: 0;
	border-radius: 5px;
	overflow: hidden;
	width: 334px;
	height: 374px;
	visibility: visible;
}

.footer{
	position:absolute;
	left: 0;
	right: 0;
	bottom: 0;
	clear: both;
	line-height: 36px;
	text-align: center;
	color: #b6b6b6;
	background-color: #eff4fa;
	border-top: 1px solid #d6dfea;
	font-size: 12px;
	width: 100%;
}

a {
	color:#1d5494;
	text-decoration: none;
}
a:hover{
color:#551A8B;
}
.header_login{

	width: 100%;
	overflow: hidden;
	height: 50px;
	border-bottom: 1px solid #c0cdd9;
	background-color: #f9fbfe;
	clear: both;
	line-height: 50px;
	color: #999;
	position: relative;

}

.header_login .active{
	/*background-image: url(../Image/LoginNew/yaoshi.png);
	background-repeat: no-repeat;*/
	color:#000;
}

.header_login .active .yaoshi{
	width: 30px;
	height: 30px;
	float: left;
	margin-top: 10px;
	margin-left: 30px;
	background-image: url(../Image/LoginNew/yaoshi.png);
	background-repeat: no-repeat;
	color:#000;
}

.active_bottom{
	border-bottom: 1px solid #465767;
	width: 136px;
	position: absolute;
	bottom: 0px;
	_bottom: -2px;
	left: 36px;

}

.login_btn{
	float: left;
	cursor: pointer;

}

.input{
	background: none;
	width: 280px;
	text-align: center;
	margin: 36px auto;
}

.showPlaceholder .placeholder {
	visibility: visible;
	cursor: text;
}
.placeholder {
	color: #C9C9C9;
	font-size: 14px;
	position: absolute;
	left: 30px;
	top: 14px;
	line-height: 14px;
	visibility: hidden;
	background: none;
}

.col-6{
	width: 50%;
}

.col-3{
	width: 33%;
}

.col-12{
	width: 100%;
}

.text-center{
	text-align: center;
}

.inputOuter{
	height:40px;
	border: 0px solid #96A5B4;
	background-color:#FFF;
	width: 100%;
}

.uinArea,.pwdArea,.verifyArea{
	height: 50px;
	position: relative;
	z-index: 10;
}
.toolbar{
	text-align: left;
	width: 100%;
	margin-top: 15px;
	margin-left:28px;
	position: relative;
	z-index: 10;
}
.toolbar a{
	text-decoration: none;
	font-size:12px;
	margin-right: 1.5em;
}
.toolbar a>img{
	position: static;
    width: 1em;
    height: .825em;
    border: none;
    vertical-align: center;
}
.inputstyle{
		top: 0px;
		padding: 8px 38px 7px 10px;
		left: 0px;
		border-radius: 3px;
		width: 232px;
		margin-top:3px;
		background-color:#FFF;
		font-family:'微软雅黑';
		font-size:14px;
		border: 1px solid #96A5B4;
		outline: 0px;
		border-radius: 3px;
}

.inputstyle_focus{
	border:1px solid #4892e7;
	outline: 0;
}
.inputstyle:focus{
	border:1px solid #4892e7;
	outline: 0;
}


.input_tips{
	/*display: none;*/
	position: absolute;
	top: 13px;
	left: 13px;
	font-size: 14px;
	line-height: 16px;
	color: #aaa;
	cursor: text;
}

#submit{
	height:47px;
	width: 100%;
	line-height:47px;
	border-radius:2px;
	margin-top:20px;
	background-color:#5A98DE;
	font-family:'微软雅黑';
	border:none;
	font-size:15px;
	cursor:pointer;
	text-align:center;
	color:#FFF;
	position:relative;
	z-index:1;
}

.login_text{
	font-size: 13px;
}
.br_20{
	height: 20px;
}
.input_hide{
	display: none;
}
.input_block{
	display: block;
}
