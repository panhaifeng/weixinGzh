.bar{
	width:25px;
	height:105px;
	position:absolute;
	right:-25px; top:-1px;
	background:url(../Image/mini_bg.png) no-repeat;
	display:block;
}


.rollboder {
	BORDER: #bcbcbc 1px solid;
	position: absolute;
	bottom: 37px;
	right: 0px;
	background: rgba(255, 255, 255, 0.5);
}
.rollleft {
	FLOAT: left;
	WIDTH: 400px;
}
.rollleft STRONG {
	COLOR: #10377F;
}
.rollcenter {
	BORDER-RIGHT: #bcbcbc 1px solid;
	BORDER-LEFT: #bcbcbc 1px solid;
	WIDTH: 30px; TEXT-ALIGN: center;
}
.rollright {
	FLOAT: right;
	WIDTH: 40px;
	TEXT-ALIGN: center;
}
.rollright IMG {
	VERTICAL-ALIGN: middle;
}
